    "class Exponents:\n",
    "    def __init__(self, a = 1, b = 1):\n",
    "        self.a = a\n",
    "        self.b = b\n",
    "    def __pow__(self, other):\n",
    "        a = self.a ** other.a\n",
    "        b = self.b ** other.b\n",
    "        return(a, b)\n",
    "e1 = Exponents(4, 2)\n",
    "e2 = Exponents(2, 2)\n",
    "print(e1 ** e2)"
   ]
